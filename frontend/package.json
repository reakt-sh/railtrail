        "@angular/localize": "^20.3.15",
        "@capacitor/cli": "^6.2.0",
        "@eslint/js": "^9.13.0",
        "@types/eslint__js": "^8.42.3",
        "@types/jasmine": "~5.1.0",
        "angular-eslint": "19.0.0",
        "eslint": "^9.16.0",
        "jasmine-core": "~5.1.0",
        "json-schema-to-typescript": "^15.0.3",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
