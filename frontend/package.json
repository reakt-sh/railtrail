{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "debug-build": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "serve": "ng serve --configuration development",
    "test": "ng test",
    "sme": "ng build --source-map=true --named-chunks==true && source-map-explorer dist/frontend/**/*.js",
    "capacitor:sync:after": "rsync -ph --copy-links --ignore-errors --progress ./runtime-environment.mobile.js ./android/app/src/main/assets/public/runtime-environment.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.3.2",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/material": "^17.3.2",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@capacitor/android": "^6.1.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/core": "latest",
    "@capacitor/ios": "^6.1.0",
    "@maplibre/ngx-maplibre-gl": "^17.4.2",
    "@turf/turf": "^6.5.0",
    "json-schema-to-typescript": "^14.0.0",
    "maplibre-gl": "^4.1.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular/cli": "^17.1.2",
    "@angular/compiler-cli": "^17.1.0",
    "@capacitor/cli": "latest",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.3.2"
  }
}
